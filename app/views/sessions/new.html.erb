<div class="container">
  <header><h1 id="log_in">Login</h1></header>
  <br>
  <%= form_tag url: login_path do %>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
    <br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br>
    <%= submit_tag 'Log in' %>
  <% end %>
</div>