<p id="notice"><%= notice %></p>

<h1>Listing Study Spots</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_spots.each do |study_spot| %>
      <tr>
        <td><%= link_to 'Show', study_spot %></td>
        <td><%= link_to 'Edit', edit_study_spot_path(study_spot) %></td>
        <td><%= link_to 'Destroy', study_spot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study spot', new_study_spot_path %>
